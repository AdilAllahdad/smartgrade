# Frontend Environment Variables (Example)
# Copy this file to .env and update with your values
# Vite requires all client-side env variables to be prefixed with VITE_

# API Base URL - Backend server URL (without /api suffix)
# For local development: http://localhost:5000
# For production: Your deployed backend URL (e.g., https://your-backend.onrender.com)
VITE_API_BASE_URL=http://localhost:5000

# Environment
VITE_ENV=development
